{
  "name": "wowow",
  "version": "2.8.2",
  "description": "a cli tool for install ESlint, prettier, lint-staged and commitlint quickly",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "changelog": "standard-version && conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog:first": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "deploy": "bash ./deploy.sh",
    "commit": "npx git-cz"
  },
  "author": "jelee",
  "license": "MIT",
  "keywords": [
    "ESlint",
    "prettier",
    "lint-staged",
    "commitlint",
    "project init",
    "integration"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JE-lee/wowow.git"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^3.0.2",
    "cross-env": "^7.0.2",
    "cross-spawn": "^7.0.1",
    "fs-extra": "^9.0.0",
    "fs-sync": "^1.0.6",
    "globby": "^11.0.0",
    "make-dir": "^3.0.0",
    "micromatch": "^4.0.2",
    "yaml": "^1.8.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "chai": "^4.2.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.31",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "^6.5.1",
    "husky": "^4.2.3",
    "mocha": "^6.2.1",
    "standard-version": "^7.1.0"
  },
  "bin": {
    "wowow": "./src/install.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
